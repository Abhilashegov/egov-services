From 0ad87a68a051ae14a7bba21461480e5a1d242fe3 Mon Sep 17 00:00:00 2001
From: Phani Kiran Gutha <phani.kiran@walkingtree.tech>
Date: Fri, 11 Aug 2017 13:23:20 +0530
Subject: [PATCH] Changed the host property names in application.properties
 file

---
 .../org/egov/tradelicense/common/config/PropertiesManager.java    | 8 ++++----
 .../tl-services/src/main/resources/application.properties         | 8 ++++----
 2 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/tradelicense/tl-services/src/main/java/org/egov/tradelicense/common/config/PropertiesManager.java b/tradelicense/tl-services/src/main/java/org/egov/tradelicense/common/config/PropertiesManager.java
index 093735a..094e191 100644
--- a/tradelicense/tl-services/src/main/java/org/egov/tradelicense/common/config/PropertiesManager.java
+++ b/tradelicense/tl-services/src/main/java/org/egov/tradelicense/common/config/PropertiesManager.java
@@ -14,16 +14,16 @@ public class PropertiesManager {
 	@Value("${app.timezone}")
 	private String appTimeZone;
 	
-	@Value("${egov.services.tl-masters/v1.hostname}")
+	@Value("${egov.services.tl-masters_v1.hostname}")
 	private String tradeLicenseMasterServiceHostName;
 	
-	@Value("${egov.services.tl-masters/v1.basepath}")
+	@Value("${egov.services.tl-masters_v1.basepath}")
 	private String tradeLicenseMasterServiceBasePath;
 
-	@Value("${egov.services.tl-masters/v1.documenttype.searchpath}")
+	@Value("${egov.services.tl-masters_v1.documenttype.searchpath}")
 	private String documentServiceSearchPath;
 	
-	@Value("${egov.services.tl-masters/v1.category.searchpath}")
+	@Value("${egov.services.tl-masters_v1.category.searchpath}")
 	private String categoryServiceSearchPath;
 	
 	@Value("${egov.services.egov-location.hostname}")
diff --git a/tradelicense/tl-services/src/main/resources/application.properties b/tradelicense/tl-services/src/main/resources/application.properties
index f9adc9f..9db0b02 100755
--- a/tradelicense/tl-services/src/main/resources/application.properties
+++ b/tradelicense/tl-services/src/main/resources/application.properties
@@ -9,14 +9,14 @@ spring.datasource.password=postgres
 
 
 #tl-masters service
-egov.services.tl-masters/v1.hostname=http://localhost:8080/
-egov.services.tl-masters/v1.basepath=tl-masters/v1/
+egov.services.tl-masters_v1.hostname=http://localhost:8080/
+egov.services.tl-masters_v1.basepath=tl-masters/v1/
 
 #documenttype search path
-egov.services.tl-masters/v1.documenttype.searchpath=documenttype/_search
+egov.services.tl-masters_v1.documenttype.searchpath=documenttype/_search
 
 #category search path
-egov.services.tl-masters/v1.category.searchpath=category/_search
+egov.services.tl-masters_v1.category.searchpath=category/_search
 
 #location service
 egov.services.egov-location.hostname=http://localhost:8080/
-- 
